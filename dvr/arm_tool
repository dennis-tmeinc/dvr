
# cross compile toolchain
ifeq ($(EAGLE),eagle34)
CROSS_COMPILE_PREFIX=/home/dennis/proj/eagle34/hiktoolchain/bin/arm_v5t_le-
else ifeq ($(EAGLE),eagle32)
CROSS_COMPILE_PREFIX=/home/dennis/proj/eagle32/hik_toolchain/bin/arm-linux-
else ifeq ($(EAGLE),mv)
CROSS_COMPILE_PREFIX=/home/dennis/bin/cross-arm-mv5sft/bin/arm-mv5sft-linux-gnueabi-
else
CROSS_COMPILE_PREFIX=
endif

export CROSS_COMPILE=$(CROSS_COMPILE_PREFIX)
export CC=$(CROSS_COMPILE_PREFIX)gcc
export STRIP=$(CROSS_COMPILE_PREFIX)strip

# debugging folder
debug_folder=/home/dennis/nfsroot/eagletest
